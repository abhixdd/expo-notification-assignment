{
  "expo": {
    "name": "Expo Notifications",
    "slug": "expo-notification-assignment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.notificationassignment",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "package": "com.expo.notificationassignment",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "useNextNotificationsApi": false,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "networkSecurityConfig": {
        "domainConfig": [
          {
            "domain": ["*"],
            "config": {
              "cleartextTrafficPermitted": true
            }
          }
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3B82F6",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "333bfcd2-1401-4b23-9dda-df0cab90b6c4"
      }
    },
    "owner": "abhixd"
  }
}
